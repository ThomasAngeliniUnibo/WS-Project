@prefix : <http://www.semanticweb.org/health-track-ontology#> .
@prefix mu: <https://w3id.org/italia/onto/MU/> .
@prefix ti: <https://w3id.org/italia/onto/TI/> .
@prefix cpv: <https://w3id.org/italia/onto/CPV/> .
@prefix fma: <http://purl.obolibrary.org/obo/fma.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/health-track-ontology#> .

<http://www.semanticweb.org/health-track-ontology#> rdf:type owl:Ontology ;
                                                     owl:imports <http://purl.obolibrary.org/obo/ws-fse-prototype> ,
                                                                 <https://w3id.org/italia/onto/CPV> .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/italia/onto/MU/hasMeasureType
mu:hasMeasureType rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/MU/hasMeasurementUnit
mu:hasMeasurementUnit rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/MU/hasValue
mu:hasValue rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/MU/value
mu:value rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/TI/atTime
ti:atTime rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/TI/date
ti:date rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/TI/endTime
ti:endTime rdf:type owl:AnnotationProperty .


###  https://w3id.org/italia/onto/TI/startTime
ti:startTime rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.obolibrary.org/obo/ws-fse-prototype#hasDoneExamination
<http://purl.obolibrary.org/obo/ws-fse-prototype#hasDoneExamination> rdf:type owl:ObjectProperty .


###  http://purl.obolibrary.org/obo/ws-fse-prototype#hasExaminationRecord
<http://purl.obolibrary.org/obo/ws-fse-prototype#hasExaminationRecord> rdf:type owl:ObjectProperty .


###  http://purl.obolibrary.org/obo/ws-fse-prototype#hasMedicalExamination
<http://purl.obolibrary.org/obo/ws-fse-prototype#hasMedicalExamination> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/health-track-ontology#analysisHasDoctor
:analysisHasDoctor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   owl:inverseOf :doctorHasDoneAnalysis ;
                   rdfs:label "analysis has doctor"@en .


###  http://www.semanticweb.org/health-track-ontology#atTimeInstant
:atTimeInstant rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf ti:hasTemporalEntity ;
               rdfs:range ti:TimeInstant ;
               rdfs:comment "Relazione tra qualsiasi cosa e un istante di tempo."@it ;
               rdfs:label "at time instant" .


###  http://www.semanticweb.org/health-track-ontology#doctorHasDoneAnalysis
:doctorHasDoneAnalysis rdf:type owl:ObjectProperty ;
                       owl:propertyChainAxiom ( <http://purl.obolibrary.org/obo/ws-fse-prototype#hasDoneExamination>
                                                :hasAnalysis
                                              ) ;
                       rdfs:label "doctor has done analysis"@en .


###  http://www.semanticweb.org/health-track-ontology#hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasEpisode ;
             rdfs:range :Activity ;
             rdfs:label "has activity"@en .


###  http://www.semanticweb.org/health-track-ontology#hasAnalysis
:hasAnalysis rdf:type owl:ObjectProperty ,
                      owl:InverseFunctionalProperty ;
             rdfs:domain <http://purl.obolibrary.org/obo/ws-fse-prototype#MedicalExamination> ;
             rdfs:range :Analysis ;
             rdfs:label "has analysis"@en .


###  http://www.semanticweb.org/health-track-ontology#hasCardioActivity
:hasCardioActivity rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasActivity ;
                   rdfs:range :Cardio ;
                   rdfs:label "has cardio activity"@en .


###  http://www.semanticweb.org/health-track-ontology#hasEpisode
:hasEpisode rdf:type owl:ObjectProperty ;
            owl:inverseOf :isEpisodeOf ;
            rdfs:domain cpv:Person ;
            rdfs:range :Episode ;
            rdfs:label "has episode" .


###  http://www.semanticweb.org/health-track-ontology#hasMeasureAggregator
:hasMeasureAggregator rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain mu:Value ;
                      rdfs:range :Measure_Aggregator ;
                      rdfs:label "measure aggregator"@en .


###  http://www.semanticweb.org/health-track-ontology#hasPhysicalEntity
:hasPhysicalEntity rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isPhysicalEntityOfAnalysis ;
                   rdfs:domain :Analysis ;
                   rdfs:range <http://purl.obolibrary.org/obo/FMA_61775> ;
                   rdfs:label "has physical entity"@en .


###  http://www.semanticweb.org/health-track-ontology#hasSnapshot
:hasSnapshot rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasEpisode ;
             rdfs:range :Snapshot ;
             rdfs:label "has snapshot"@en .


###  http://www.semanticweb.org/health-track-ontology#hasTracked
:hasTracked rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :isTrackedBy ;
            rdfs:domain :Device ;
            rdfs:range :Episode ;
            rdfs:label "has tracked"@en .


###  http://www.semanticweb.org/health-track-ontology#hasWalkingActivity
:hasWalkingActivity rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCardioActivity ;
                    rdfs:range :Walking ;
                    rdfs:label "has walking activity"@en .


###  http://www.semanticweb.org/health-track-ontology#isActivityOf
:isActivityOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isEpisodeOf ;
              rdfs:label "is activity of"@en .


###  http://www.semanticweb.org/health-track-ontology#isAnalysisOfPerson
:isAnalysisOfPerson rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isEpisodeOf ;
                    owl:inverseOf :personHasAnalysis ;
                    rdfs:label "is analysis of person"@en .


###  http://www.semanticweb.org/health-track-ontology#isBloodAnalysisOfPerson
:isBloodAnalysisOfPerson rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :isAnalysisOfPerson ;
                         owl:inverseOf :personHasBloodAnalysis ;
                         rdfs:domain :BloodTest ;
                         rdfs:label "is blood analysis of person"@en .


###  http://www.semanticweb.org/health-track-ontology#isCardioActivityOf
:isCardioActivityOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isActivityOf ;
                    rdfs:label "is cardio activity of"@en .


###  http://www.semanticweb.org/health-track-ontology#isEpisodeOf
:isEpisodeOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:label "is episode of" .


###  http://www.semanticweb.org/health-track-ontology#isPhysicalEntityOfAnalysis
:isPhysicalEntityOfAnalysis rdf:type owl:ObjectProperty ;
                            rdfs:label "is physical entity of analysis"@en .


###  http://www.semanticweb.org/health-track-ontology#isSnapshotOf
:isSnapshotOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isEpisodeOf ;
              rdfs:label "is snapshot of"@en .


###  http://www.semanticweb.org/health-track-ontology#isTrackedBy
:isTrackedBy rdf:type owl:ObjectProperty ;
             rdfs:label "is tracked by"@en .


###  http://www.semanticweb.org/health-track-ontology#isWalkingActivityOf
:isWalkingActivityOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :isCardioActivityOf ;
                     rdfs:label "is walking activty of"@en .


###  http://www.semanticweb.org/health-track-ontology#medicalRecordHasAnalysis
:medicalRecordHasAnalysis rdf:type owl:ObjectProperty ;
                          owl:propertyChainAxiom ( <http://purl.obolibrary.org/obo/ws-fse-prototype#hasExaminationRecord>
                                                   <http://purl.obolibrary.org/obo/ws-fse-prototype#hasMedicalExamination>
                                                   :hasAnalysis
                                                 ) ;
                          rdfs:label "medical record has analysis"@en .


###  http://www.semanticweb.org/health-track-ontology#personHasAnalysis
:personHasAnalysis rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasEpisode ;
                   owl:propertyChainAxiom ( <http://purl.obolibrary.org/obo/ws-fse-prototype#hasMedicalRecord>
                                            <http://purl.obolibrary.org/obo/ws-fse-prototype#hasExaminationRecord>
                                            <http://purl.obolibrary.org/obo/ws-fse-prototype#hasMedicalExamination>
                                            :hasAnalysis
                                          ) ;
                   rdfs:label "person has analysis"@en .


###  http://www.semanticweb.org/health-track-ontology#personHasBloodAnalysis
:personHasBloodAnalysis rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :personHasAnalysis ;
                        rdfs:range :BloodTest ;
                        rdfs:label "person has blood analysis"@en .


###  https://w3id.org/italia/onto/TI/hasTemporalEntity
ti:hasTemporalEntity rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/health-track-ontology#steps
:steps rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Walking ;
       rdfs:range xsd:int .


#################################################################
#    Classes
#################################################################

###  http://purl.obolibrary.org/obo/FMA_61775
<http://purl.obolibrary.org/obo/FMA_61775> rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/ws-fse-prototype#MedicalExamination
<http://purl.obolibrary.org/obo/ws-fse-prototype#MedicalExamination> rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/ws-fse-prototype#PersonaDataSet
<http://purl.obolibrary.org/obo/ws-fse-prototype#PersonaDataSet> rdf:type owl:Class .


###  http://www.semanticweb.org/health-track-ontology#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :Episode ,
                          ti:TimeIndexedEvent ;
          owl:disjointWith :Snapshot .


###  http://www.semanticweb.org/health-track-ontology#Analysis
:Analysis rdf:type owl:Class ;
          rdfs:subClassOf :Episode ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :atTimeInstant ;
                            owl:someValuesFrom ti:TimeInstant
                          ] .


###  http://www.semanticweb.org/health-track-ontology#Biopsy
:Biopsy rdf:type owl:Class ;
        rdfs:subClassOf :Analysis .


###  http://www.semanticweb.org/health-track-ontology#BloodTest
:BloodTest rdf:type owl:Class ;
           rdfs:subClassOf :Analysis .


###  http://www.semanticweb.org/health-track-ontology#Cardio
:Cardio rdf:type owl:Class ;
        rdfs:subClassOf :Activity .


###  http://www.semanticweb.org/health-track-ontology#Device
:Device rdf:type owl:Class .


###  http://www.semanticweb.org/health-track-ontology#Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf <https://w3id.org/italia/onto/RO/Role> .


###  http://www.semanticweb.org/health-track-ontology#Episode
:Episode rdf:type owl:Class ;
         rdfs:subClassOf <https://w3id.org/italia/onto/l0/Entity> .


###  http://www.semanticweb.org/health-track-ontology#Measure_Aggregator
:Measure_Aggregator rdf:type owl:Class ;
                    rdfs:comment "Aggregatori di misure (es: min/max/avg)"@it ;
                    rdfs:label "Measure Aggregator"@en .


###  http://www.semanticweb.org/health-track-ontology#Snapshot
:Snapshot rdf:type owl:Class ;
          rdfs:subClassOf :Episode ,
                          <https://w3id.org/italia/onto/l0/Characteristic> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :atTimeInstant ;
                            owl:someValuesFrom ti:TimeInstant
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty mu:hasValue ;
                            owl:someValuesFrom mu:Value
                          ] .


###  http://www.semanticweb.org/health-track-ontology#Ultrasound
:Ultrasound rdf:type owl:Class ;
            rdfs:subClassOf :Analysis .


###  http://www.semanticweb.org/health-track-ontology#Walking
:Walking rdf:type owl:Class ;
         rdfs:subClassOf :Cardio .


###  https://w3id.org/italia/onto/MU/MeasureType
mu:MeasureType rdf:type owl:Class .


###  https://w3id.org/italia/onto/MU/MeasurementUnit
mu:MeasurementUnit rdf:type owl:Class .


###  https://w3id.org/italia/onto/RO/Role
<https://w3id.org/italia/onto/RO/Role> rdf:type owl:Class .


###  https://w3id.org/italia/onto/TI/TimeIndexedEvent
ti:TimeIndexedEvent rdf:type owl:Class .


###  https://w3id.org/italia/onto/TI/TimeInstant
ti:TimeInstant rdf:type owl:Class .


